{
  "expo": {
    "name": "Contador de Billetes",
    "slug": "contador-billetes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/contador-billetes.png",
    "scheme": "contadorbilletes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/contador-billetes.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.emir09.contadorbilletes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/contador-billetes.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "90811414-7146-4abd-98e0-975a135f85a8"
      }
    }
  }
}
